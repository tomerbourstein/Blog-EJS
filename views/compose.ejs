<%- include("partials/header.ejs"); -%>

<h1>Compose</h1>

<form action="/compose" name="newPost" method="post">
  <div class="form-group">
    <label for="postTitle">Title</label>
    <input class="form-control" type="text" name="postTitle">
  </div>
  <div class="form-group">
    <label for="postText">Post</label>
    <textarea class="form-control" name="postText" rows="5"></textarea>
  </div>
    <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>

